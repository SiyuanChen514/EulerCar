<?xml version="1.0" ?>
<robot name="hirobot_burger"
  xmlns:xacro="http://ros.org/wiki/xacro">

<!--与环视小车适配-->
<!--double_camera和imu待定-->

  <!--base_footprint为小车底盘中心在地面的投影-->
  <link name="base_footprint">
    <visual>
      <geometry>
        <box size="0.001 0.001 0.001"/>
      </geometry>
    </visual>
  </link>

  <!--base_link为底盘-->
  <link name="base_link">
    <visual>
      <origin xyz="0 0 0.0" rpy="0 0 0"/>
      <geometry>
        <box size="0.14 0.111 0.0436"/>
      </geometry>
      <material name="white">
      	<color rgba="1 1 1 0.9"/>
      </material>
    </visual>
  </link>

  <link name="wheel_left1_link">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <cylinder radius="0.033" length="0.0262"/>
      </geometry>
      <material name="black">
	    <color rgba="0 0 0 0.9"/>
      </material>
    </visual>
  </link>

  <link name="wheel_right1_link">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <cylinder radius="0.033" length="0.0262"/>
      </geometry>
      <material name="black">
	    <color rgba="0 0 0 0.9"/>
      </material>
    </visual> 
  </link> 

  <link name="box_mid">
    <visual>
      <origin xyz="0 0 0.0" rpy="0 0 0"/>
      <geometry>
        <box size="0.093 0.092 0.05"/>
      </geometry>
      <material name="white">
      	<color rgba="1 1 1 0.9"/>
      </material>
    </visual>
  </link>

  <link name="box_top">
    <visual>
      <origin xyz="0 0 0.0" rpy="0 0 0"/>
      <geometry>
        <box size="0.05 0.05 0.05"/>
      </geometry>
      <material name="white">
      	<color rgba="1 1 1 0.9"/>
      </material>
    </visual>
  </link>

  <link name="laser_base">
    <visual>
      <origin xyz="0 0 0.0" rpy="0 0 0"/>
      <geometry>
        <box size="0.035 0.035 0.0196"/>
      </geometry>
      <material name="white">
      	<color rgba="1 1 1 0.9"/>
      </material>
    </visual>
  </link>

  <link name="laser">
    <visual>
      <origin xyz="0 0 0.0" rpy="0 0 0"/>
      <geometry>
        <cylinder radius="0.01925" length="0.0144"/>
      </geometry>
      <material name="black">
      	<color rgba="0 0 0 0.9"/>
      </material>
    </visual>
  </link>

  <!--三个单目相机底座-->
  <link name="camera_base_left">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <box size="0.038 0.01 0.038"/>
      </geometry>
      <material name="black">
      	<color rgba="0 0 0 0.9"/>
      </material>
    </visual>
  </link>
  <link name="camera_base_right">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <box size="0.038 0.01 0.038"/>
      </geometry>
      <material name="black">
      	<color rgba="0 0 0 0.9"/>
      </material>
    </visual>
  </link>
  <link name="camera_base_rear">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <box size="0.038 0.01 0.038"/>
      </geometry>
      <material name="black">
      	<color rgba="0 0 0 0.9"/>
      </material>
    </visual>
  </link>

  <!--三个单目相机-->
  <link name="camera_left">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <cylinder radius="0.014" length="0.023"/>
      </geometry>
      <material name="black">
      	<color rgba="0 0 0 0.9"/>
      </material>
    </visual>
  </link>
  <link name="camera_right">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <cylinder radius="0.014" length="0.023"/>
      </geometry>
      <material name="black">
      	<color rgba="0 0 0 0.9"/>
      </material>
    </visual>
  </link>
  <link name="camera_rear">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <cylinder radius="0.014" length="0.023"/>
      </geometry>
      <material name="black">
      	<color rgba="0 0 0 0.9"/>
      </material>
    </visual>
  </link>

  <!--双目相机-->
  <link name="double_camera_link">
    <visual>
      <origin xyz="0.014 0.088 0.03" rpy="0 1.57 0"/>
      <geometry>
        <box size="0 0 0"/>
      </geometry>
      <material name="black">
      	<color rgba="0 0 0 0.9"/>
      </material>
    </visual>
  </link>

  <!--joint-->
  <joint name="base_joint" type="fixed">
    <parent link="base_footprint"/>
    <child link="base_link"/>
    <origin xyz="0 0 0.0442" rpy="0 0 0"/>
  </joint>
  
  <joint name="wheel_left1_joint" type="continuous">
    <parent link="base_link"/>
    <child link="wheel_left1_link"/>
    <origin xyz="0.04 0.0686 -0.0112" rpy="1.57 0 0"/>
    <axis xyz="0 0 1"/>
  </joint>

  <joint name="wheel_right1_joint" type="continuous">
    <parent link="base_link"/>
    <child link="wheel_right1_link"/>
    <origin xyz="0.04 -0.0686 -0.0112" rpy="1.57 0 0"/>
    <axis xyz="0 0 1"/>
  </joint>

  <joint name="base_mid_joint" type="fixed">
    <parent link="base_link"/>
    <child link="box_mid"/>
    <origin xyz="0 0 0.044" rpy="0 0 0"/>
  </joint>

  <joint name="mid_top_joint" type="fixed">
    <parent link="box_mid"/>
    <child link="box_top"/>
    <origin xyz="0 0 0.05" rpy="0 0 0"/>
  </joint>

  <joint name="top_laser_base_joint" type="fixed">
    <parent link="box_top"/>
    <child link="laser_base"/>
    <origin xyz="0 0 0.0348" rpy="0 0 0"/>
  </joint>

  <joint name="laser_joint" type="fixed">
    <parent link="laser_base"/>
    <child link="laser"/>
    <origin xyz="0 0 0.017" rpy="0 0 0"/>
  </joint>

  <!--三个单目相机底座joint-->
  <joint name="camera_base_left_joint" type="fixed">
    <parent link="box_top"/>
    <child link="camera_base_left"/>
    <origin xyz="0 0.03 0" rpy="0 0 0"/>
  </joint>
  <joint name="camera_base_right_joint" type="fixed">
    <parent link="box_top"/>
    <child link="camera_base_right"/>
    <origin xyz="0 -0.03 0" rpy="0 0 0"/>
  </joint>
  <joint name="camera_base_rear_joint" type="fixed">
    <parent link="box_top"/>
    <child link="camera_base_rear"/>
    <origin xyz="-0.03 0 0" rpy="0 0 1.57"/>
  </joint>

  <!--三个个单目相机joint-->
  <joint name="camera_left_joint" type="fixed">
    <parent link="camera_base_left"/>
    <child link="camera_left"/>
    <origin xyz="0 0.0165 0" rpy="1.57 0 0"/>
  </joint>
  <joint name="camera_right_joint" type="fixed">
    <parent link="camera_base_right"/>
    <child link="camera_right"/>
    <origin xyz="0 -0.0165 0" rpy="-1.57 0 0"/>
  </joint>
  <joint name="camera_rear_joint" type="fixed">
    <parent link="camera_base_rear"/>
    <child link="camera_rear"/>
    <origin xyz="0 0.0165 0" rpy="1.57 1.57 0"/>
  </joint>

  <!--双目相机joint-->
  <joint name="double_camera_joint" type="fixed">
    <parent link="base_link"/>
    <child link="double_camera_link"/>
    <origin xyz="0.07 0 0.055" rpy="0 0 0"/>
  </joint>

</robot>
